<?xml version="1.0" ?>

<!-- Unit test for ceation of remote env with bidirectional  Vertex
     communication. "Down" and "Up" remote env agent are used
     -->


<!DOCTYPE node SYSTEM "../etc/objspec.dtd"> 

<node id="test" parent="Vert">
    <node id="v1" parent="Vert"/>
    <node id="v2" parent="Vert"/>
    <node id="e1" parent="Edge"/>
    <node id="Renv" parent="ARenv">
	<cont id="Remote_Uid" val="Local"/>
	<node id="renv_root" parent="ARenvu">
	    <cont val="Connect"/>
	    <node id="remote_node_1" parent="Vert">
	    </node>
	    <node id="remote_node_2" parent="./../../v1">
	    </node>
	</node>
    </node>
    <cont targ="./e1/P1" ref="./../../v1"/>
    <cont targ="./e1/P2" ref="./../../Renv/renv_root/remote_node_1"/>
</node>

